<div class="mb-16 mt-6">
  <%= render "railsui/shared/sticky_subhead", title: "Pagination", subtitle: "Simple" %>

<%= render preview do %>
<nav aria-label="Page navgiation" class="text-slate-500 sm:text-sm text-base font-medium">
  <ul class="flex flex-wrap items-center sm:justify-between justify-center">
    <li class="flex-1 flex justify-start items-center sm:w-auto w-full">
      <a data-controller="prevent" data-action="click->prevent#prevent" href="#" class="flex justify-center items-center space-x-2 py-2 px-4 rounded hover:bg-slate-50/50 group hover:text-indigo-600 sm:w-auto w-full sm:bg-transparent bg-slate-50 sm:mb-0 mb-3">
        <%= icon "arrow-small-left", classes: "w-4 h-4 text-slate-400 group-hover:text-indigo-500" %>
        <span>Previous</span>
      </a>
    </li>
    <div class="flex items-center justify-center sm:w-auto w-full sm:text-sm text-lg">
      <li>
        <a data-controller="prevent" data-action="click->prevent#prevent" href="#" class="py-2 px-4 block border-t-2 border-transparent rounded hover:bg-slate-50/50">1</a>
      </li>
      <li>
        <a data-controller="prevent" data-action="click->prevent#prevent" href="#" class="py-2 px-4 border-t-2 block border-indigo-500 hover:bg-indigo-50/50 text-indigo-600">2</a>
      </li>
      <li>
        <a data-controller="prevent" data-action="click->prevent#prevent" href="#" class="py-2 px-4 block border-t-2 border-transparent rounded hover:bg-slate-50/50">3</a>
      </li>
      <li>
        <span href="#" class="py-2 px-4 block border-t-2 border-transparent rounded pointer-events-none select-none">&hellip;</span>
      </li>
       <li>
        <a data-controller="prevent" data-action="click->prevent#prevent" href="#" class="py-2 px-4 block border-t-2 border-transparent rounded hover:bg-slate-50/50">8</a>
      </li>
       <li>
        <a data-controller="prevent" data-action="click->prevent#prevent" href="#" class="py-2 px-4 block border-t-2 border-transparent rounded hover:bg-slate-50/50">9</a>
      </li>
       <li>
        <a data-controller="prevent" data-action="click->prevent#prevent" href="#" class="py-2 px-4 block border-t-2 border-transparent rounded hover:bg-slate-50/50">10</a>
      </li>

    </div>
    <li class="flex-1 flex justify-end items-center">
      <a data-controller="prevent" data-action="click->prevent#prevent" href="#" class="flex justify-center items-center space-x-2 py-2 px-4 rounded hover:bg-slate-50/50 group hover:text-indigo-600 sm:w-auto w-full sm:bg-transparent bg-slate-50 sm:mt-0 mt-3">
        <span>Next</span>
        <%= icon "arrow-small-right", classes: "w-4 h-4 text-slate-400 group-hover:text-indigo-500" %>
      </a>
    </li>
  </ul>
</nav>
<% end %>

